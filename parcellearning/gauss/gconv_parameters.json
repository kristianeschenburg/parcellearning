{
    "model": "GAUSS",

    "data": {
        "in": "/projects3/parcellation/",
        "out": "/projects3/parcellation/models/gauss/primarysensory/",
        "training": "/projects3/parcellation/data/regional/primarysensory/training.bin",
        "validation": "/projects3/parcellation/data/regional/primarysensory/validation.bin",
        "testing": "/projects3/parcellation/data/regional/primarysensory/validation.bin"
    },

    "features": [
        "regionalized",
        "spectral",
        "curv",
        "myelin",
        "sulcal"
    ],

    "model_parameters": {
        "in_dim": 80,
        "num_classes": 181,
        "num_kernels": [8,8],
        "num_layers": 3,
        "num_hidden": 32,
        "feat_drop": 0.2,
        "krnl_drop": 0.2,
        "negative_slope": 0.2,
        "activation": "torch.nn.functional.leaky_relu",
        "allow_zero_in_degree": "True"
    },

    "optimizer_parameters": {
        "lr": 0.005,
        "weight_decay": 5e-4
    },

    "training_parameters": {
        "epochs": 600,
        "n_batch": 4,
        "verbose": "False"
    },

    "stopping_parameters": {
        "mode": "lower",
        "patience": 20
    }
}